{
  "name": "@lfx-pcc/shared",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./interfaces": {
      "types": "./dist/interfaces/index.d.ts",
      "import": "./dist/interfaces/index.js",
      "require": "./dist/interfaces/index.js",
      "default": "./dist/interfaces/index.js"
    },
    "./enums": {
      "types": "./dist/enums/index.d.ts",
      "import": "./dist/enums/index.js",
      "require": "./dist/enums/index.js",
      "default": "./dist/enums/index.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.js",
      "require": "./dist/constants/index.js",
      "default": "./dist/constants/index.js"
    },
    "./validators": {
      "types": "./dist/validators/index.d.ts",
      "import": "./dist/validators/index.js",
      "require": "./dist/validators/index.js",
      "default": "./dist/validators/index.js"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "build:development": "tsc",
    "build:production": "tsc",
    "build:staging": "tsc",
    "watch": "tsc --watch",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@angular/forms": "^19.0.0",
    "@fullcalendar/core": "^6.1.19",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "@angular/forms": "^19.0.0",
    "@fullcalendar/core": "^6.1.19"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0"
  }
}
